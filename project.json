{
  "version": "1.0.10",
  "description": "The .net client for the ThisData Login Intelligence API. Add an extra layer of security to your app and get notified of suspicious login behaviour",
  "authors": [ "ThisData" ],
  "tags": ["security","login","analytics","anomaly detection"],
  "projectUrl": "https://github.com/revertio/thisdata-dotnet",
  "licenseUrl": "",
  "tooling": {
    "defaultNamespace": "ThisData"
  },

  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": ["DEBUG", "TRACE"]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": ["RELEASE", "TRACE"],
        "optimize": true
      }
    }
  },

 "dependencies": {
      "System.Net.Http": "4.0.0",
      "Newtonsoft.Json": "8.0.2"
  },

  "frameworks": {
    "net45": {},
    "dnxcore50": {}
  }
}
